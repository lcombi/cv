@article{roedig2014OBSERVATIONAL,
  title = {{{OBSERVATIONAL SIGNATURES OF BINARY SUPERMASSIVE BLACK HOLES}}},
  author = {Roedig, Constanze and Krolik, Julian H. and Miller, M. Coleman},
  year = {2014},
