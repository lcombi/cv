@article{noble2009DIRECT,
  title = {{{DIRECT CALCULATION OF THE RADIATIVE EFFICIENCY OF AN ACCRETION DISK AROUND A BLACK HOLE}}},
  author = {Noble, Scott C. and Krolik, Julian H. and Hawley, John F.},
  year = {2009},
