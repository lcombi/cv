@article{penna2010Simulations,
  title = {Simulations of {{Magnetized Disks Around Black Holes}}: {{Effects}} of {{Black Hole Spin}}, {{Disk Thickness}}, and {{Magnetic Field Geometry}}},
  shorttitle = {Simulations of {{Magnetized Disks Around Black Holes}}},
  author = {Penna, Robert F. and McKinney, Jonathan C. and Narayan, Ramesh and Tchekhovskoy, Alexander and Shafee, Rebecca and McClintock, Jeffrey E.},
  year = {2010},
  month = oct,
  journal = {Mon. Not. R. Astron. Soc.},
  volume = {408},
  number = {2},
