@article{mewes2016Numerical,
  title = {Numerical Relativity Simulations of Thick Accretion Disks around Tilted {{Kerr}} Black Holes},
  author = {Mewes, Vassilios and Font, Jos{\'e} A. and Galeazzi, Filippo and Montero, Pedro J. and Stergioulas, Nikolaos},
  year = {2016},
  month = mar,
  journal = {Phys. Rev. D},
  volume = {93},
  number = {6},
  eprint = {1506.04056},
  eprinttype = {arxiv},
  pages = {064055},
  issn = {2470-0010, 2470-0029},
  doi = {10.1103/PhysRevD.93.064055},
  abstract = {In this work we present 3D numerical relativity simulations of thick accretion disks around tilted Kerr BH. We investigate the evolution of three different initial disk models with a range of initial black hole spin magnitudes and tilt angles. For all the disk-to-black hole mass ratios considered (0.044-0.16) we observe significant black hole precession and nutation during the evolution. This indicates that for such mass ratios, neglecting the self-gravity of the disks by evolving them in a fixed background black hole spacetime is not justified. We find that the two more massive models are unstable against the Papaloizou-Pringle (PP) instability and that those PP-unstable models remain unstable for all initial spins and tilt angles considered, showing that the development of the instability is a very robust feature of such PP-unstable disks. Our lightest model, which is the most astrophysically favorable outcome of mergers of binary compact objects, is stable. The tilt between the black hole spin and the disk is strongly modulated during the growth of the PP instability, causing a partial global realignment of black hole spin and disk angular momentum in the most massive model with constant specific angular momentum l. For the model with non-constant l-profile we observe a long-lived m=1 non-axisymmetric structure which shows strong oscillations of the tilt angle in the inner regions of the disk. This effect might be connected to the development of Kozai-Lidov oscillations. Our simulations also confirm earlier findings that the development of the PP instability causes the long-term emission of large amplitude gravitational waves, predominantly for the l=m=2 multipole mode. The imprint of the BH precession on the gravitational waves from tilted BH-torus systems remains an interesting open issue that would require significantly longer simulations than those presented in this work.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  file = {/home/lcombi/Zotero/storage/V57G6F3Y/mewes2016.pdf;/home/lcombi/Zotero/storage/XZNPV4UH/1506.html}
}

@article{meyer-vernet1987Physics,
