  author = {Ajith, P. and Boyle, Michael and Brown, Duncan A. and Br{\"u}gmann, Bernd and Buchman, Luisa T. and Cadonati, Laura and Campanelli, Manuela and Chu, Tony and Etienne, Zachariah B. and Fairhurst, Stephen and Hannam, Mark and Healy, James and Hinder, Ian and Husa, Sascha and Kidder, Lawrence E. and Krishnan, Badri and Laguna, Pablo and Liu, Yuk Tung and London, Lionel and Lousto, Carlos O. and Lovelace, Geoffrey and MacDonald, Ilana and Marronetti, Pedro and Mohapatra, Satya and M{\"o}sta, Philipp and M{\"u}ller, Doreen and Mundim, Bruno C. and Nakano, Hiroyuki and Ohme, Frank and Paschalidis, Vasileios and Pekowsky, Larne and Pollney, Denis and Pfeiffer, Harald P. and Ponce, Marcelo and P{\"u}rrer, Michael and Reifenberger, George and Reisswig, Christian and Santamar{\'i}a, Luc{\'i}a and Scheel, Mark A. and Shapiro, Stuart L. and Shoemaker, Deirdre and Sopuerta, Carlos F. and Sperhake, Ulrich and Szil{\'a}gyi, B{\'e}la and Taylor, Nicholas W. and Tichy, Wolfgang and Tsatsin, Petr and Zlochower, Yosef},
  year = {2012},
  month = jun,
  journal = {Class. Quantum Grav.},
  volume = {29},
  number = {12},
  eprint = {1201.5319},
  eprinttype = {arxiv},
  pages = {124001},
  issn = {0264-9381, 1361-6382},
  doi = {10.1088/0264-9381/29/12/124001},
  abstract = {The Numerical INJection Analysis (NINJA) project is a collaborative effort between members of the numerical relativity and gravitational wave data analysis communities. The purpose of NINJA is to study the sensitivity of existing gravitational-wave search and parameter-estimation algorithms using numerically generated waveforms, and to foster closer collaboration between the numerical relativity and data analysis communities. The first NINJA project used only a small number of injections of short numerical-relativity waveforms, which limited its ability to draw quantitative conclusions. The goal of the NINJA-2 project is to overcome these limitations with long post-Newtonian - numerical relativity hybrid waveforms, large numbers of injections, and the use of real detector data. We report on the submission requirements for the NINJA-2 project and the construction of the waveform catalog. Eight numerical relativity groups have contributed 63 hybrid waveforms consisting of a numerical portion modelling the late inspiral, merger, and ringdown stitched to a post-Newtonian portion modelling the early inspiral. We summarize the techniques used by each group in constructing their submissions. We also report on the procedures used to validate these submissions, including examination in the time and frequency domains and comparisons of waveforms from different groups against each other. These procedures have so far considered only the \$(\textbackslash ell,m)=(2,2)\$ mode. Based on these studies we judge that the hybrid waveforms are suitable for NINJA-2 studies. We note some of the plans for these investigations.},
  archiveprefix = {arXiv},
  keywords = {General Relativity and Quantum Cosmology},
  file = {/home/lcombi/Zotero/storage/QBD5PKH7/1201.html}
}

